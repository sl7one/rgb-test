{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "importOrder": ["^\\u0000", "^react", "^@?\\w", "^\\.", "^\\u0000"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
